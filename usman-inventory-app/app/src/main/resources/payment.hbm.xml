<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.usman.medical.data.Payment" table="payment">
    <!-- Primary key --> 
    <id name="moduleId" column="pay_id" />
    
    <property name="patientId"  column="patient_id" />
    <property name="feeType"    column="fee_type"/>
    <property name="amount"     column="amount"/>
    <property name="department" column="department" not-null="false"/>
    <property name="doctorId"   column="doctor_id"  not-null="false"/>
    <property name="description" column="description" not-null="false"/>
    <property name="createdBy"/>
    <property name="createdDate" type="calendar"/>
  </class>
</hibernate-mapping>
