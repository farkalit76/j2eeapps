<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.farkalit.mapping.model.VoterAddress" table="adress">
        <meta attribute="class-description">This class contains the student's address details.</meta>
        <id name="addId" type="long" column="ADD_ID">
            <generator class="native" />
        </id>
        <property name="addStreet" column="ADD_STREET" type="string" length="45" />
        <property name="addCity" column="ADD_CITY" type="string" length="45" />
        <property name="addState" column="ADD_STATE" type="string" length="45" />
        <property name="addZipCode" column="ADD_ZIPCODE" type="string" length="6" />
    </class>
</hibernate-mapping>
